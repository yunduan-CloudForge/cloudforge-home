{
  "name": "vite-plugin-trae-solo-badge",
  "version": "1.0.0",
  "type": "module",
  "description": "TRAE Badge component - A beautiful 'Powered by TRAE' badge for web applications",
  "keywords": [
    "badge",
    "trae",
    "component",
    "vite-plugin",
    "ui",
    "web-component"
  ],
  "author": "czy88840616",
  "license": "MIT",
  "main": "dist/trae-badge.js",
  "module": "dist/trae-badge.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "echo \"No tests yet\"",
    "lint": "eslint src --ext .ts,.js",
    "size": "size-limit"
  },
  "exports": {
    ".": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.esm.js",
      "require": "./dist/vite-plugin.js"
    },
    "./badge": {
      "types": "./dist/index.d.ts",
      "import": "./dist/trae-badge.esm.js",
      "require": "./dist/trae-badge.js"
    },
    "./vite": {
      "types": "./dist/vite-plugin.d.ts",
      "import": "./dist/vite-plugin.esm.js",
      "require": "./dist/vite-plugin.js"
    },
    "./style.css": "./dist/trae-badge.css"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.10.0",
    "rollup": "^4.6.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-svg": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "size-limit": "^10.0.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "vite": ">=4.0.0"
  },
  "size-limit": [
    {
      "path": "dist/trae-badge.js",
      "limit": "5 KB"
    }
  ],
  "dependencies": {
    "tslib": "^2.8.1"
  }
