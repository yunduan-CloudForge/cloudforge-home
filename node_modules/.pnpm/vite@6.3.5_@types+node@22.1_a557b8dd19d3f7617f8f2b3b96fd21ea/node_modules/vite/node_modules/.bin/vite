#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_a557b8dd19d3f7617f8f2b3b96fd21ea/node_modules/vite/bin/node_modules:/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_a557b8dd19d3f7617f8f2b3b96fd21ea/node_modules/vite/node_modules:/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_a557b8dd19d3f7617f8f2b3b96fd21ea/node_modules:/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_a557b8dd19d3f7617f8f2b3b96fd21ea/node_modules/vite/bin/node_modules:/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_a557b8dd19d3f7617f8f2b3b96fd21ea/node_modules/vite/node_modules:/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/vite@6.3.5_@types+node@22.1_a557b8dd19d3f7617f8f2b3b96fd21ea/node_modules:/mnt/d/云锻开源社区/云锻开源团队/Trae_CloudForge-home/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
